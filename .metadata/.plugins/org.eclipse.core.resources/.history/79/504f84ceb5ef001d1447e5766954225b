<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/merging_more_to_one1" elementFormDefault="qualified" targetNamespace="http://www.example.org/merging_more_to_one1">

    <complexType name="food">
    	<sequence>
    		<element name="name" type="normalizedString"/>
    		<element name="price" type="string"/>
    		<element name="description" type="string"/>
    		<element name="calories" type="string"/>
    	</sequence>
    </complexType>

    <element name="food" type="tns:food"/>

    <complexType name="breakfast_menu">
    	<sequence>
    		<element maxOccurs="unbounded" minOccurs="0" name="food" type="tns:food"/>
    	</sequence>
    </complexType>

    <element name="breakfast_menu" type="tns:breakfast_menu"/>
</schema>
