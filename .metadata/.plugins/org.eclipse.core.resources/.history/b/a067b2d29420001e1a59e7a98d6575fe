                  <xs:annotation>
                    <xs:documentation source="Description" xml:lang="en">This request message is a subsequent request based on the previous message sent in this transaction.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
