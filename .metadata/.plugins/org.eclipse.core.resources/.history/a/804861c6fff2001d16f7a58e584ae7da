<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/coersions" xmlns:tns="http://www.example.org/coersions" elementFormDefault="qualified">

    <complexType name="orders">
    	<sequence>
    		<element name="name" type="string"></element>
    		<element name="orderno" type="string"></element>
    		<element name="orderamount" type="string"></element>
    	</sequence>
    </complexType>

    <element name="orders" type="tns:orders"></element>

    <complexType name="creditlimit">
    	<sequence>
    		<element name="accountno" type="string"></element>
    		<element name="accounttype" type="string"></element>
    		<element name="branchname" type="string"></element>
    		<element name="amount" type="string"></element>
    	</sequence>
    </complexType>

    <element name="creditlimit" type="tns:creditlimit"></element>

    <complexType name="coerse">
    	<choice>
    		<choice>
    			<annotation>
    				<appinfo>
    					<tns:creditlimit></tns:creditlimit>
    					<tns:orders></tns:orders>
    				</appinfo>
    			</annotation>
    		</choice>
    	</choice>
    </complexType>

    <element name="coerse" type="tns:coerse"></element>
</schema>