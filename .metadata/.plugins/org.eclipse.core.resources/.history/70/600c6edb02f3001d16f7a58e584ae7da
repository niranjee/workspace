<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/coersions" elementFormDefault="qualified" targetNamespace="http://www.example.org/coersions">

    <complexType name="orders">
    	<sequence>
    		<element name="name" type="string"/>
    		<element name="orderno" type="string"/>
    		<element name="orderamount" type="string"/>
    	</sequence>
    </complexType>

    <element name="orders" type="tns:orders"/>

    <complexType name="creditlimit">
    	<sequence>
    		<element name="accountno" type="string"/>
    		<element name="accounttype" type="string"/>
    		<element name="branchname" type="string"/>
    		<element name="amount" type="string"/>
    	</sequence>
    </complexType>

    <element name="creditlimit" type="tns:creditlimit"/>

    <complexType name="coerse">
    	<choice>
    		<choice>
    			<annotation>
    				<appinfo>
    					<tns:creditlimit/>
    					<tns:orders/>
    				</appinfo>
    			</annotation>
    		</choice>
    	</choice>
    </complexType>

    <element name="coerse"/>




</schema>
