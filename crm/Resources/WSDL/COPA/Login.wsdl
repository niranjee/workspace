                <xs:attribute name="Relation" type="ns0:StringLength1to32">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Indicates the type of relationship with the person or company in the profile, such as Spouse, Child, Family, Business Associate, Interest Group, Medical, Security,Other, etc.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ShareMarketInd">
