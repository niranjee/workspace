                                    <xs:documentation source="Description" xml:lang="en">This indicates that an actual payment has been made.</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="reserve">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">This indicates that a hold for the indicated amount has been placed on a credit card or that a cash amount has been taken from the customer to guarantee final payment.</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="refund">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">This indicates that the payment amount of this PaymentDetail element is for a refund.</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
