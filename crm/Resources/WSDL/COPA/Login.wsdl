                                                                <xs:documentation source="Example" xml:lang="en">Example: acG@26!w</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:element>
                                                        <xs:element form="qualified" minOccurs="0" name="Secure" type="ns0:EncryptionTokenType">
                                                            <xs:annotation>
                                                                <xs:documentation source="Description" xml:lang="en">Secure representation.</xs:documentation>
                                                                <xs:documentation source="Example" xml:lang="en">Example: AEGHV234AUD54367 (tokenized)</xs:documentation>
                                                                <xs:documentation source="Example" xml:lang="en">Example: 5dfc52b51bd35553df8592078de921bc (encrypted)</xs:documentation>
                                                                <xs:documentation source="Example" xml:lang="en">Example: xxxxxxxxxxxx4444 (masked)</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:element>
                                                    </xs:choice>
