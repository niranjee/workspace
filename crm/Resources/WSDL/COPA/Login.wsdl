                                    <xs:documentation source="Description" xml:lang="en">The quantity of loyalty units being redeemed.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" name="MiscChargeOrder">
