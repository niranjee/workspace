            <xs:complexType name="CoverageLimitType">
                <xs:annotation>
                    <xs:documentation source="Description" xml:lang="en">Information about the insurance coverage limits that applies to all covered travelers</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element form="qualified" minOccurs="0" name="Deductible">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">The amount of out-of-pocket expense a covered individual must pay per claim.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:attribute name="Amount" type="ns0:Money">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">A monetary amount.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="CurrencyCode" type="ns0:AlphaLength3">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">An ISO 4217 (3) alpha character code that specifies a monetary unit.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">The ISO 4217 standard "minor unit" for the number of decimal places for a particular currency.</xs:documentation>
                                    <xs:documentation source="Implementer" xml:lang="en">Implementer: The ISO 4217 standard "minor unit" is typically used when the amount provided includes the minor unit of currency without a decimal point (e.g., USD 8500 needs DecimalPlaces="2" to represent $85).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="PolicyLimit">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">The maximum currency amount of available coverage for the policy.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:attribute name="Amount" type="ns0:Money">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">A monetary amount.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="CurrencyCode" type="ns0:AlphaLength3">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">An ISO 4217 (3) alpha character code that specifies a monetary unit.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">The ISO 4217 standard "minor unit" for the number of decimal places for a particular currency.</xs:documentation>
                                    <xs:documentation source="Implementer" xml:lang="en">Implementer: The ISO 4217 standard "minor unit" is typically used when the amount provided includes the minor unit of currency without a decimal point (e.g., USD 8500 needs DecimalPlaces="2" to represent $85).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="IndividualLimit">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">The maximum amount of available coverage for a covered individual.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:attribute name="Amount" type="ns0:Money">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">A monetary amount.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="CurrencyCode" type="ns0:AlphaLength3">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">An ISO 4217 (3) alpha character code that specifies a monetary unit.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">The ISO 4217 standard "minor unit" for the number of decimal places for a particular currency.</xs:documentation>
                                    <xs:documentation source="Implementer" xml:lang="en">Implementer: The ISO 4217 standard "minor unit" is typically used when the amount provided includes the minor unit of currency without a decimal point (e.g., USD 8500 needs DecimalPlaces="2" to represent $85).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" ref="ns0:TPA_Extensions">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">A placeholder in the schema to allow for additional elements and attributes to be included per Trading Partner Agreement (TPA).</xs:documentation>
                            <xs:documentation source="Implementer" xml:lang="en">Implementer: It is recommended that you submit comments to enhance the OpenTravel specification to accommodate any trading partner extensions that you use.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="CoverageLevel" type="ns0:StringLength1to16">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Indicates a level of coverage for the plan benefit e.g. Silver, Gold, Platinum</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="CoverageType" type="ns0:OTA_CodeType" use="required">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Identifies the type of coverage or benefit requested by the covered individuals. Refer to OpenTravel Code List Coverage Type (COV).</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Covered" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Flag to indicate coverage type is available/requested.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="EffectiveDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Indicates the starting date.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ExpireDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Indicates the ending date.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ExpireDateExclusiveInd" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">When true, indicates that the ExpireDate is the first day after the applicable period (e.g. when expire date is Oct 15  the last date of the period is Oct 14).</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="UnlimitedCoverage" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation source="Description" xml:lang="en">Flag to indicate unlimited coverage for the specific benefit is available/requested.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:complexType>
            <xs:complexType name="CustomerType">
                <xs:annotation>
                    <xs:documentation source="Description" xml:lang="en">Contains basic data on the customer's identity, location, relationships, finances, memberships, etc.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element form="qualified" maxOccurs="5" minOccurs="0" name="PersonName" type="ns0:PersonNameType">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">Detailed name information for the customer.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" maxOccurs="99" minOccurs="0" name="Telephone">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">Information on a telephone number for the customer.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:attribute name="AreaCityCode" type="ns0:NumericStringLength1to8">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Code assigned for telephones in a specific region, city, or area.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="CountryAccessCode" type="ns0:NumericStringLength1to3">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Code assigned by telecommunications authorities for international country access identifier.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="DefaultInd" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">When true, indicates a default value should be used.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="EffectiveDate" type="xs:date">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Indicates the starting date.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="ExpireDate" type="xs:date">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Indicates the ending date.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="ExpireDateExclusiveInd" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">When true, indicates that the ExpireDate is the first day after the applicable period (e.g. when expire date is Oct 15  the last date of the period is Oct 14).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="Extension" type="ns0:NumericStringLength1to5">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Extension to reach a specific party at the phone number.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="FormattedInd" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Specifies if the associated data is formatted or not. When true, then it is formatted; when false, then not formatted.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="PIN" type="ns0:StringLength1to8">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Additional codes used for pager or telephone access rights.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="ParentCompanyRef" type="ns0:StringLength1to8">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">A reference to the company from which this element has been inherited.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="PhoneLocationType" type="ns0:OTA_CodeType">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Describes the location of the phone, such as Home, Office, Property Reservation Office, etc. Refer to OpenTravel Code List Phone Location Type (PLT).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="PhoneNumber" type="ns0:StringLength1to32" use="required">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Telephone number assigned to a single location.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="PhoneTechType" type="ns0:OTA_CodeType">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Indicates type of technology associated with this telephone number, such as Voice, Data, Fax, Pager, Mobile, TTY, etc. Refer to OpenTravel Code List Phone Technology Type (PTT).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="PhoneUseType" type="ns0:OTA_CodeType">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Describes the type of telephone number, in the context of its general use (e.g. Home, Business, Emergency Contact, Travel Arranger, Day, Evening). Refer to OpenTravel Code List Phone Use Type (PUT).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="RPH" type="ns0:RPH_Type">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Used elsewhere in the message to reference a specific telephone number (including faxes).</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="Remark" type="ns0:StringLength1to128">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">A remark associated with the telephone number.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="ShareMarketInd">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Permission for sharing data for marketing purposes.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:annotation>
                                        <xs:documentation source="Description" xml:lang="en"> If the value=Inherit, specifies data sharing permissions for marketing purposes.</xs:documentation>
                                    </xs:annotation>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="Yes"/>
                                        <xs:enumeration value="No"/>
                                        <xs:enumeration value="Inherit"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="ShareSynchInd">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Permission for sharing data for synchronization of information held by other travel service providers.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:annotation>
                                        <xs:documentation source="Description" xml:lang="en"> If the value=Inherit, specifies data sharing permissions for synchronization of information held by other travel service providers.</xs:documentation>
                                    </xs:annotation>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="Yes"/>
                                        <xs:enumeration value="No"/>
                                        <xs:enumeration value="Inherit"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="TransferAction" type="ns0:TransferActionType">
                                <xs:annotation>
                                    <xs:documentation source="Description" xml:lang="en">Indicates under what conditions the element will be transfered to the booking.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" maxOccurs="99" minOccurs="0" name="Email">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">Information on an email address for the customer.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="ns0:EmailType">
                                    <xs:attribute name="ParentCompanyRef" type="ns0:StringLength1to8">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">A reference to the company from which this element has been inherited.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="TransferAction" type="ns0:TransferActionType">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">Indicates under what conditions the element will be transfered to the booking.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" maxOccurs="99" minOccurs="0" name="Address">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">Detailed information on an address for the customer.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="ns0:AddressInfoType">
                                    <xs:sequence>
                                        <xs:element form="qualified" minOccurs="0" name="CompanyName" type="ns0:CompanyNameType">
                                            <xs:annotation>
                                                <xs:documentation source="Description" xml:lang="en">Identifies a company.</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element form="qualified" minOccurs="0" name="AddresseeName" type="ns0:PersonNameType">
                                            <xs:annotation>
                                                <xs:documentation>Name of the person to whom this address relates.</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="EffectiveDate" type="xs:date">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">Indicates the starting date.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ExpireDate" type="xs:date">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">Indicates the ending date.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ExpireDateExclusiveInd" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">When true, indicates that the ExpireDate is the first day after the applicable period (e.g. when expire date is Oct 15  the last date of the period is Oct 14).</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ParentCompanyRef" type="ns0:StringLength1to8">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">A reference to the company from which this element has been inherited.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="TransferAction" type="ns0:TransferActionType">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">Indicates under what conditions the element will be transfered to the booking.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ValidationStatus">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">Indicates if the address has been validated or not and if it has been successful or not.</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="SystemValidated">
                                                    <xs:annotation>
                                                        <xs:documentation source="Description" xml:lang="en">Indicates the address has been validated automatically.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:enumeration>
                                                <xs:enumeration value="UserValidated">
                                                    <xs:annotation>
                                                        <xs:documentation source="Description" xml:lang="en">Indicates the address has been validated by the agent.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:enumeration>
                                                <xs:enumeration value="NotChecked">
                                                    <xs:annotation>
                                                        <xs:documentation source="Description" xml:lang="en">Indicates the address has not been validated.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:enumeration>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" maxOccurs="5" minOccurs="0" name="URL">
                        <xs:annotation>
                            <xs:documentation source="Description" xml:lang="en">Information on a URL for the customer.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="ns0:URL_Type">
                                    <xs:attribute name="TransferAction" type="ns0:TransferActionType">
                                        <xs:annotation>
                                            <xs:documentation source="Description" xml:lang="en">Indicates under what conditions this element will be transfered to the booking.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element form="qualified" maxOccurs="3" minOccurs="0" name="CitizenCountryName">
