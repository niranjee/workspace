<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/minimum" xmlns:tns="http://www.example.org/minimum" elementFormDefault="qualified">

    <complexType name="minimum_values">
    	<sequence>
    		<element name="mini" type="string" minOccurs="0" maxOccurs="unbounded"></element>
