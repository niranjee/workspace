							<xs:documentation>Name of the Service Configuration Item.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element maxOccurs="1" minOccurs="0" name="parentConfigurationItem" nillable="true" type="invsvc:ServiceConfigurationItemType">
						<xs:annotation>
							<xs:documentation>Parent Configuration Item information.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element maxOccurs="unbounded" minOccurs="0" name="configurationItems" nillable="true" type="invsvc:ServiceConfigurationItemType">
						<xs:annotation>
							<xs:documentation>Child configuration items, if any.</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="resourceAssignment" nillable="true" type="invsvc:ResourceAssignmentType">
						<xs:annotation>
							<xs:documentation>Resource Assignment information, if any.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="resourceReference" nillable="true" type="invsvc:ResourceReferenceType">
						<xs:annotation>
							<xs:documentation>Resource Reference, if any.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element maxOccurs="unbounded" minOccurs="0" name="characteristics" nillable="true" type="invchar:CharacteristicType">
						<xs:annotation>
							<xs:documentation>Service Configuration Item characteristics.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ResourceAssignmentType">
		<xs:annotation>
			<xs:documentation>ResourceAssignmentType that holds resource assignment information.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="state" nillable="false" type="invsvc:AssignmentStateEnum">
						<xs:annotation>
							<xs:documentation>Assignment state.</xs:documentation>
						</xs:annotation>                    
					</element>
                    <element maxOccurs="1" minOccurs="1" name="resource" nillable="false" type="invsvc:ResourceType">
						<xs:annotation>
							<xs:documentation>Resource information that is used to uniquely identify a resource.</xs:documentation>
						</xs:annotation>                    
					</element>
                </sequence>
