    <!-- ==== Message Types ==== -->

    <complexType name="ServiceFulfillmentRequestType">
		<xs:annotation>
			<xs:documentation>ServiceFulfillmentRequestType defines the base type for Requests that will be used for Service Fulfillment. All the 
			attributes that are defined at the Service Fulfillemnt request level are defined here.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invcom:InventoryRequestType">
                <sequence>
                    <element default="true" minOccurs="1" name="expandedResponse" nillable="false" type="xsd:boolean">
						<xs:annotation>
							<xs:documentation>expandedResponse determines of the ServiceType in the response should contain Configuration Items (along with child 
							configuration items). If true, the whole Service tree is returned. If false, Configuration Items are omitted. Default is false.</xs:documentation>
						</xs:annotation>                      
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CaptureServiceConfigurationInputsRequestType">
		<xs:annotation>
			<xs:documentation>CaptureServiceConfigurationInputsRequestType holds the information to create/update a Service based on some inputs. The request 
			holds the Order, Service, Party, Role and Place information.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invsvc:ServiceFulfillmentRequestType">
                <sequence>
                    <element minOccurs="1" name="orderData" nillable="false" type="invsvc:OrderDataType">
						<xs:annotation>
							<xs:documentation>Order data holds the order information.</xs:documentation>
						</xs:annotation>                      
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CaptureServiceConfigurationInputsResponseType">
		<xs:annotation>
			<xs:documentation>CaptureServiceConfigurationInputsResponseType holds the response of the CaptureServiceConfigurationInputs Web Service 
			operation. This return the created/updated Service tree.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invcom:InventoryResponseType">
                <sequence>
                    <element minOccurs="0" name="service" nillable="false" type="invsvc:ServiceType">
						<xs:annotation>
							<xs:documentation>Service information with/without configuration items.</xs:documentation>
						</xs:annotation>                      
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CaptureServiceConfigurationInputsFaultType">
	    <xs:annotation>
			<xs:documentation>Fault that is specific to Capture Service Configuration Inputs WS operation.</xs:documentation>
		</xs:annotation>  
        <complexContent>
            <extension base="invfault:InventoryFaultType"/>
        </complexContent>
    </complexType>

    <complexType name="AutomateServiceConfigurationRequestType">
		<xs:annotation>
			<xs:documentation>AutomateServiceConfigurationRequestType holds the information to process Service configuration Automation. It contains the 
			criteria to identify Service and Service Configuration.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invsvc:ServiceFulfillmentRequestType">
                <sequence>
                    <element minOccurs="1" name="serviceCriteria" nillable="false" type="invsvc:ServiceCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Service</xs:documentation>
						</xs:annotation>                      
                    </element>
                    <element minOccurs="1" name="serviceConfigurationCriteria" nillable="false" type="invsvc:ServiceConfigurationCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Service Configuration</xs:documentation>
						</xs:annotation>                      
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AutomateServiceConfigurationResponseType">
		<xs:annotation>
			<xs:documentation>AutomateServiceConfigurationResponseType holds the response of the CaptureServiceConfigurationInputs Web Service 
			operation. This return the created/updated Service tree.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invcom:InventoryResponseType">
                <sequence>
                    <element minOccurs="0" name="service" nillable="false" type="invsvc:ServiceType">
						<xs:annotation>
							<xs:documentation>Service information with/without configuration items.</xs:documentation>
						</xs:annotation>                      
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AutomateServiceConfigurationFaultType">
		<xs:annotation>
			<xs:documentation>Fault type to return any business faults within Automate Service Configuration Web Service.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invfault:InventoryFaultType"/>
        </complexContent>
    </complexType>

    <complexType name="UpdateServiceConfigurationRequestType">
		<xs:annotation>
			<xs:documentation>UpdateServiceConfigurationRequestType holds the request for Update Service Configuration Web Service operation. It contains 
			information for allocation/de-allocation of Service Configuration Items as well as characteristics of Service Configuration and Service 
			Configuration Items. </xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invsvc:ServiceFulfillmentRequestType">
                <sequence>
                    <element minOccurs="1" name="orderData" nillable="false" type="invsvc:OrderDataType">
						<xs:annotation>
							<xs:documentation>orderData contains the Order Information.</xs:documentation>
						</xs:annotation>  
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="UpdateServiceConfigurationResponseType">
		<xs:annotation>
			<xs:documentation>UpdateServiceConfigurationResponseType holds the response of the CaptureServiceConfigurationInputs Web Service 
			operation. This return the created/updated Service tree.</xs:documentation>
		</xs:annotation>      
        <complexContent>
            <extension base="invcom:InventoryResponseType">
                <sequence>
                    <element minOccurs="0" name="service" nillable="false" type="invsvc:ServiceType">
						<xs:annotation>
							<xs:documentation>Service information with/without configuration items.</xs:documentation>
						</xs:annotation>                    
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="UpdateServiceConfigurationFaultType">
		<xs:annotation>
			<xs:documentation>Fault type to return any business faults within Update Service Configuration Web Service.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invfault:InventoryFaultType"/>
        </complexContent>
    </complexType>

    <complexType name="CalculateConfigurationDifferencesRequestType">
		<xs:annotation>
			<xs:documentation>CalculateConfigurationDifferencesRequestType holds the request information for Calculate Configuration Differences 
			Web Service operation. It contains the information to identify the two Service Configurations(source and target) that have to be compared.
			</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ServiceFulfillmentRequestType">
                <sequence>
                    <element minOccurs="1" name="serviceCriteria" nillable="false" type="invsvc:ServiceCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Service.</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="1" name="sourceServiceConfigurationCriteria" nillable="false" type="invsvc:ServiceConfigurationCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Source Service Configuration version.</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="1" name="targetServiceConfigurationCriteria" nillable="false" type="invsvc:ServiceConfigurationCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Target Service Configuration version.</xs:documentation>
						</xs:annotation>                    
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CalculateConfigurationDifferencesResponseType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:InventoryResponseType">
                <sequence>
                    <element minOccurs="1" name="service" nillable="false" type="invsvc:ServiceType">
						<xs:annotation>
							<xs:documentation>Service information with/without configuration items.</xs:documentation>
						</xs:annotation>                    
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CalculateConfigurationDifferencesFaultType">
		<xs:annotation>
			<xs:documentation>Fault type to return any business faults within Calculate Configuration Differences Web Service.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invfault:InventoryFaultType"/>
        </complexContent>
    </complexType>

