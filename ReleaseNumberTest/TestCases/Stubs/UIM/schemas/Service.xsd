        </complexContent>
    </complexType>
    <complexType name="ResourceReferenceType">
		<xs:annotation>
			<xs:documentation>Resource Reference information that is used to uniquely identify a resource reference.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="resource" nillable="false" type="invsvc:ResourceType">
						<xs:annotation>
							<xs:documentation>Resource information that is used to uniquely identify a resource.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ResourceType">
		<xs:annotation>
			<xs:documentation>Resource information that is used to uniquely identify a resource.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element minOccurs="1" name="id" nillable="false" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Resource Id</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="name" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Resource Name</xs:documentation>
						</xs:annotation>                    
					</element>
                    <element minOccurs="0" name="description" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Resource Description</xs:documentation>
						</xs:annotation>                    
					</element>
                    <element minOccurs="1" name="type" nillable="false" type="invsvc:ResourceTypeEnum">
						<xs:annotation>
							<xs:documentation>Type of resource. Like (EQUIPMENT, PIPE)</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="1" name="specification" nillable="false" type="invspec:SpecificationType">
						<xs:annotation>
							<xs:documentation>Resource specification information.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ==== Data Types ==== -->
    <complexType name="OrderDataType">
		<xs:annotation>
			<xs:documentation>OrderDataType holds Order information</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element minOccurs="1" name="orderHeaderData" nillable="false" type="invsvc:OrderHeaderDataType">
						<xs:annotation>
							<xs:documentation>Order Header information.</xs:documentation>
						</xs:annotation> 
					</element>                   
                    <element minOccurs="1" name="orderDetailData" nillable="false" type="invsvc:OrderDetailDataType">
						<xs:annotation>
							<xs:documentation>Order Detail information.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="OrderHeaderDataType">
		<xs:annotation>
			<xs:documentation>Order Header information</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element minOccurs="1" name="orderId" nillable="false" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Order Id</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="1" name="orderDate" nillable="false" type="xsd:dateTime">
						<xs:annotation>
							<xs:documentation>Order Date</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="1" name="orderType" nillable="false" type="invsvc:OrderTypeEnum">
						<xs:annotation>
							<xs:documentation>Order type</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="OrderDetailDataType">
		<xs:annotation>
			<xs:documentation>Order Detail information.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element minOccurs="1" name="orderAction" nillable="false" type="invsvc:OrderActionEnum">
						<xs:annotation>
							<xs:documentation>Action that needs to be performed for the order.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="1" name="party" nillable="false" type="invparty:PartyDataType">
						<xs:annotation>
							<xs:documentation>Party that is associated with the Order.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ServiceDataType">
		<xs:annotation>
			<xs:documentation>ServiceDataType holds the Service information. This type must only be used for operation that change the Service using 
			the Service Action and Business Rules.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ServiceType">
                <sequence>
                    <element minOccurs="1" name="serviceAction" nillable="false" type="invsvc:ServiceActionEnum">
						<xs:annotation>
							<xs:documentation>Action that needs to be performed on Service.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="businessRules" nillable="true" type="invsys:RulesetType">
						<xs:annotation>
							<xs:documentation>Business rules that have to be executed for updating the Service.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ServiceConfigurationDataType">
		<xs:annotation>
			<xs:documentation>ServiceConfigurationDataType holds the Service Configuration information. This type must only be used for the operations 
			that modify a configuration using ConfigurationAction and Business Rules.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ServiceConfigurationType">
                <sequence>
                    <element minOccurs="0" name="configurationAction" nillable="true" type="invsvc:ServiceConfigurationActionEnum">
						<xs:annotation>
							<xs:documentation>Action that needs to be performed on a Service Configuration.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="businessRules" nillable="true" type="invsys:RulesetType">
						<xs:annotation>
							<xs:documentation>Business rules that have to be executed for updating the Service Configuration.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ServiceConfigurationItemDataType">
		<xs:annotation>
			<xs:documentation>ServiceConfigurationItemDataType holds the Service Configuration Item information. This type must be only used for passing 
			the configuration item information for performing allocation/-deallocation.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ServiceConfigurationItemType">
                <sequence>
                    <element name="path" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Path of the configuration item, using which it can be uniquely identified in a configuration item tree. 
				            Use index only if there are duplicate configuration item names. Index start from 0. E.g ITEM[0] indicates first occurance of a 
				            configuration item with name ITEM and ITEM[1] indicates the second occurance of ITEM in the configuration tree.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ResourceAssignmentDataType">
		<xs:annotation>
			<xs:documentation>ResourceAssignmentDataType holds Resource Assignment information. This type must only be used when requesting for 
			allocation/de-allocation of a resource.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ResourceAssignmentType">
                <sequence>
                    <element name="assignmentAction" type="invsvc:AssignmentActionEnum">
						<xs:annotation>
							<xs:documentation>Action that needs to be performed on the configuration item.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ResourceDataType">
		<xs:annotation>
			<xs:documentation>ResourceDataType holds the resource information.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ResourceType">
                <sequence>
                    <element name="quantity" type="xsd:int">
						<xs:annotation>
							<xs:documentation>Quantity of resources</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element name="criteria" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Search criteria String</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <!--
                    <element name="validations" type="invsvc:ValidationDataType" />
                    -->
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <!-- Validation is optional -->
    <!--
    <complexType name="ValidationDataType">
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element name="validationMinimum" type="xsd:string" />
                    <element name="validationUnique" type="xsd:string" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    -->

    <complexType name="ServiceConfigurationItemDifferenceDataType">
		<xs:annotation>
			<xs:documentation>ServiceConfigurationItemDifferenceDataType holds the Service Configuration Item information. This type must only be used 
			when returning a response for calculation difference for a Configuration item in two given versions. </xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ServiceConfigurationItemType">
                <sequence>
                    <element minOccurs="1" name="operation" nillable="false" type="invsvc:OperationEnum">
						<xs:annotation>
							<xs:documentation>Operation that has been performed on the source Configuration Item.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ResourceAssignmentDifferenceDataType">
		<xs:annotation>
			<xs:documentation>ResourceAssignmentDifferenceDataType holds the Resource assignment information. This type must only be used 
			when returning a response for calculation difference for a Configuration item in two given versions.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invsvc:ResourceAssignmentType">
                <sequence>
                    <element minOccurs="1" name="operation" nillable="false" type="invsvc:OperationEnum">
						<xs:annotation>
							<xs:documentation>Operation that has been performed on the Assignment.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CharacteristicDifferenceDataType">
		<xs:annotation>
			<xs:documentation>CharacteristicDifferenceDataType holds the Characteristic information. This type must only be used 
			when returning a response for calculation difference for a Configuration item in two given versions.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invchar:CharacteristicType">
                <sequence>
                    <element minOccurs="1" name="operation" nillable="false" type="invsvc:OperationEnum">
						<xs:annotation>
							<xs:documentation>Operation that has been performed on the Characteristic.</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>



    <!-- ==== Entity Criteria Types ==== -->
    <complexType name="ServiceCriteriaType">
		<xs:annotation>
			<xs:documentation>Criteria to uniquely identify a Service.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                <!-- 9887580 -->
                    <element minOccurs="0" name="serviceId" nillable="false" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Service Id</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="serviceState" nillable="false" type="invsvc:ServiceStateEnum">
						<xs:annotation>
							<xs:documentation>Current Service State</xs:documentation>
						</xs:annotation>
					</element>                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ServiceConfigurationCriteriaType">
		<xs:annotation>
			<xs:documentation>Criteria to uniquely identify a Service Configuration version.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element minOccurs="0" name="serviceConfigurationVersionNumber" nillable="false" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Service Configuration Version Number</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="serviceConfigurationId" nillable="false" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Service Configuration Id</xs:documentation>
						</xs:annotation>
					</element>                   
                    <element minOccurs="0" name="serviceConfigurationState" nillable="false" type="invsvc:ServiceConfigurationStateEnum">
						<xs:annotation>
							<xs:documentation>Current state of the Service Configuration Version.</xs:documentation>
						</xs:annotation>
					</element>                   
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ChangeStateCriteriaType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element minOccurs="0" name="serviceCriteria" nillable="false" type="invsvc:ServiceCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Service.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="1" name="serviceConfigurationCriteria" nillable="false" type="invsvc:ServiceConfigurationCriteriaType">
						<xs:annotation>
							<xs:documentation>Criteria to uniquely identify a Service Configuration version.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element name="configurationAction" type="invsvc:ServiceConfigurationActionEnum">
						<xs:annotation>
							<xs:documentation>Action that needs to be performed on the Service Configuration.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element name="stateChangePolicy" type="invsvc:StateChangePolicyEnum">
						<xs:annotation>
							<xs:documentation>State change policy for the child configuration items.</xs:documentation>
						</xs:annotation>                    
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>


    <!-- ==== Enumerations ==== -->
    <simpleType name="OrderTypeEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for Order Type</xs:documentation>
		</xs:annotation>    
        <restriction base="xsd:string">
            <enumeration value="new"/>
            <enumeration value="amend"/>
            <enumeration value="cancel"/>
        </restriction>
    </simpleType>
    <simpleType name="OrderActionEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for Order Action</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="add"/>
            <enumeration value="change"/>
            <enumeration value="disconnect"/>
        </restriction>
    </simpleType>
    <simpleType name="ServiceActionEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for  Service Action</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="none"/>
            <enumeration value="update"/>
            <enumeration value="cancel"/>
            <enumeration value="complete"/>
            <enumeration value="disconnect"/>
            <enumeration value="suspend"/>
            <enumeration value="resume"/>
            <enumeration value="cancel not issue"/>  
        </restriction>
    </simpleType>
    <simpleType name="ServiceConfigurationActionEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for Service Configuration Action.</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="none"/>
            <enumeration value="update"/>
            <enumeration value="approve"/>
            <enumeration value="issue"/>
            <enumeration value="cancel"/>
            <enumeration value="complete"/> 
            <enumeration value="add"/>
            <enumeration value="change"/>
            <enumeration value="disconnect"/>                      
        </restriction>
    </simpleType>
    <simpleType name="AssignmentActionEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for Assignment Action</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="assign"/>
            <enumeration value="unassign"/>
        </restriction>
    </simpleType>
    <simpleType name="ResourceActionEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for Resource Action</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="activate"/>
            <enumeration value="deactivate"/>
        </restriction>
    </simpleType>
    <simpleType name="ServiceStateEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for Service State</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="pending"/>
            <enumeration value="in service"/>
            <enumeration value="pending cancel"/>
            <enumeration value="cancelled"/>
            <enumeration value="pending disconnect"/>
            <enumeration value="disconnected"/>
            <enumeration value="cancel pending disconnect"/>
            <enumeration value="suspended"/>
        </restriction>
    </simpleType>
    <simpleType name="ServiceConfigurationStateEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for State of ServiceCofigurationState</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="in progress"/>
            <enumeration value="designed"/>
            <enumeration value="issued"/>
            <enumeration value="completed"/>
            <enumeration value="pending cancel"/>
            <enumeration value="cancelled"/>
        </restriction>
    </simpleType>
    <simpleType name="AssignmentStateEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for AssignmentState</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="pending assign"/>
            <enumeration value="assigned"/>
            <enumeration value="pending unassign"/>
            <enumeration value="unassigned"/>            
            <enumeration value="disconnected"/>
            <enumeration value="pending available"/>
            <enumeration value="pending unavailable"/>
            <enumeration value="ported"/>
            <enumeration value="unavailable"/>
            <enumeration value="transitional"/>
        </restriction>
    </simpleType>
    <simpleType name="ResourceTypeEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for ResourceType</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="TELEPHONE-NUMBER"/>
            <enumeration value="LOGICAL_DEVICE"/>
            <enumeration value="DEVICE_INTERFACE"/>
            <enumeration value="PHYSICAL_DEVICE"/>
            <enumeration value="CUSTOM_NETWORKADDRESS"/>
            <enumeration value="CUSTOM_OBJECT"/>
            <enumeration value="EQUIPMENT"/>
            <enumeration value="EQUIPMENT-HOLDER"/>
            <enumeration value="GEOADDRESS"/>
            <enumeration value="GEOADDRESSRANGE"/>
            <enumeration value="GEOLOCATION"/>
            <enumeration value="GEOSITE"/>
            <enumeration value="LD-ACCOUNT"/>
            <enumeration value="NETWORK"/>
            <enumeration value="PHYSICAL_CONNECTOR"/>
            <enumeration value="PHYSICAL_PORT"/>
            <enumeration value="PIPE"/>
            <enumeration value="SERVICE"/>
        </restriction>
    </simpleType>


    <simpleType name="OperationEnum"> 
		<xs:annotation>
			<xs:documentation>Defines valid values for Operation</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <enumeration value="added"/>
            <enumeration value="deleted"/>
            <enumeration value="modified"/>
            <enumeration value="none"/>
        </restriction>
    </simpleType>
    <simpleType name="StateChangePolicyEnum">
		<xs:annotation>
			<xs:documentation>Defines valid values for StateChangePolicy</xs:documentation>
		</xs:annotation>     
        <restriction base="xsd:string">
            <!-- <enumeration value="TransitionAllRelated" /> -->
            <enumeration value="TransitionThisServiceOnly"/>
        </restriction>
    </simpleType>

