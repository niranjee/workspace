    <!-- ==== Entity Value Types ==== -->
    <complexType name="ServiceType">
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                <!-- 9887580 -->
                    <element minOccurs="0" name="id" nillable="false" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Id of the Service</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="name" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Name of the Service</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="description" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Description of the Service</xs:documentation>
						</xs:annotation>                    
					</element>
                    <element minOccurs="0" name="state" nillable="true" type="invsvc:ServiceStateEnum">
						<xs:annotation>
							<xs:documentation>Current State  of the Service</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="startDate" nillable="true" type="xsd:dateTime">
						<xs:annotation>
							<xs:documentation>Start Date  of the Service</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="1" name="specification" nillable="false" type="invspec:SpecificationType">
						<xs:annotation>
							<xs:documentation>Specification information of the Service</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="characteristics" nillable="true" type="invchar:CharacteristicType">
						<xs:annotation>
							<xs:documentation>Service Characteristics</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="places" nillable="true" type="invplace:PlaceType">
						<xs:annotation>
							<xs:documentation>Places that are associated with the Service.</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="parties" nillable="true" type="invparty:PartyType">
						<xs:annotation>
							<xs:documentation>Parties that are associated with the Service.</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="configurations" nillable="true" type="invsvc:ServiceConfigurationType">
						<xs:annotation>
							<xs:documentation>Configuration Verions of a Service.</xs:documentation>
						</xs:annotation>                    
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ServiceConfigurationType">
		<xs:annotation>
			<xs:documentation>ServiceConfigurationType holds the Service Configuration Version.</xs:documentation>
		</xs:annotation>    
        <complexContent>
            <extension base="invcom:EntityValueType">
                <sequence>
                    <element default="0" minOccurs="1" name="version" nillable="false" type="xsd:int">
						<xs:annotation>
							<xs:documentation>Version number(integer)</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="previousVersion" nillable="true" type="xsd:int">
						<xs:annotation>
							<xs:documentation>Previous Service Configuration version number</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="id" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Service Configuration Id</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="name" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Name of the Service Configuration</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="description" nillable="true" type="xsd:string">
						<xs:annotation>
							<xs:documentation>Description  of the Service Configuration</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element minOccurs="0" name="state" nillable="true" type="invsvc:ServiceConfigurationStateEnum">
						<xs:annotation>
							<xs:documentation>Current state  of the Service Configuration</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element minOccurs="0" name="startDate" nillable="true" type="xsd:dateTime">
						<xs:annotation>
							<xs:documentation>Start Date  of the Service Configuration</xs:documentation>
						</xs:annotation>                    
					</element>
                    <element minOccurs="0" name="endDate" nillable="true" type="xsd:dateTime">
						<xs:annotation>
							<xs:documentation>End Date  of the Service Configuration</xs:documentation>
						</xs:annotation>                    
					</element>                    
                    <element minOccurs="1" name="configSpec" nillable="false" type="invspec:SpecificationType">
						<xs:annotation>
							<xs:documentation>Specification information of Service Configuration.</xs:documentation>
						</xs:annotation>
					</element>                    
                    <element maxOccurs="unbounded" minOccurs="0" name="configurationItems" nillable="true" type="invsvc:ServiceConfigurationItemType">
						<xs:annotation>
							<xs:documentation>Configuration Items within a Service Configuration Version.</xs:documentation>
						</xs:annotation>                    
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="characteristics" nillable="true" type="invchar:CharacteristicType">
						<xs:annotation>
							<xs:documentation>Service Configuration Version Characteristics(Characteristics of the ROOT configuration item).</xs:documentation>
						</xs:annotation>                    
					</element>
                    
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ServiceConfigurationItemType">
