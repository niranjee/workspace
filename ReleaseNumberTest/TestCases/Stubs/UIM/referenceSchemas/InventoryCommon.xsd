--><xs:schema xmlns="http://xmlns.oracle.com/communications/inventory/webservice" xmlns:appfault="http://xmlns.oracle.com/communications/framework/webservice/fault/v1-0" xmlns:invcom="http://xmlns.oracle.com/communications/inventory/webservice/common" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://xmlns.oracle.com/communications/inventory/webservice/common">
	<xs:import namespace="http://xmlns.oracle.com/communications/framework/webservice/fault/v1-0" schemaLocation="./FaultRoot.xsd"/>
	<!-- ================= Parent Classes ======================= -->
	<xs:complexType name="InventoryRequestType">
		<xs:annotation>
			<xs:documentation>Defines base type for all Requests for UIM Web Services. All common attributes of UIM requests will be part 
			of this type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="header" nillable="true" type="xsd:string">
				<xs:annotation>
					<xs:documentation>Stores Header information that is optional.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InventoryResponseType">
		<xs:annotation>
			<xs:documentation>Base type for Response used for all the Web Services Operations in UIM.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="warnings" nillable="true" type="xsd:string">
				<xs:annotation>
					<xs:documentation>Warnings, if any.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="errors" nillable="true" type="appfault:ApplicationFaultType">
				<xs:annotation>
					<xs:documentation>Errors, if Any.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="messages" nillable="true" type="xsd:string">
				<xs:annotation>
					<xs:documentation>Debug Messages, if Any and the messageConfirmation is not set.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CharacteristicDataValueType">
		<xs:annotation>
			<xs:documentation>Characteristic information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="invcom:EntityValueType">
				<xs:sequence>
					<xs:element minOccurs="0" name="name" nillable="true" type="xs:string">
						<xs:annotation>
							<xs:documentation>Characteristic Name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="value" nillable="true" type="xs:string">
						<xs:annotation>
							<xs:documentation>Characteristic value</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="MessageConfirmation">
		<xs:annotation>
			<xs:documentation>Message Confirmation</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xsd:string">
			<xs:enumeration value="ignore"/>
			<xs:enumeration value="required"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EntityKeyType">
		<xs:annotation>
			<xs:documentation>
				An id of an entity within the instance of the
				Provisioning application.Could be comination of Ids
			</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="EntityValueType">
		<xs:annotation>
			<xs:documentation>
				An id of an entity within the instance of the
				Provisioning application.Could be combination of Ids
			</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="RequestContext">
		<xs:annotation>
			<xs:documentation>
				An businessInteraction id for the Provisioning of the application.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="businessInteractionId" nillable="true" type="xsd:string">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="messageConfirmation" nillable="true" type="invcom:MessageConfirmation">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommonStubs">
		<xs:annotation>
			<xs:documentation>
				Defined Mainly For using the earlier complex definitions for  wsdl generation
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="stub1" nillable="true" type="invcom:EntityValueType"/>
			<xs:element name="stub2" nillable="true" type="invcom:InventoryRequestType"/>
			<xs:element name="stub3" nillable="true" type="invcom:InventoryResponseType"/>
			<xs:element name="stub4" nillable="true" type="invcom:MessageConfirmation"/>
			<xs:element name="stub5" nillable="true" type="invcom:EntityKeyType"/>
			<xs:element name="stub6" nillable="true" type="invcom:RequestContext"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
