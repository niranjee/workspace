	<xs:import namespace="http://eai.telkom.co.za/internal/EnterpriseMessage/20110801"/>
	<xs:complexType name="ResponseType">
		<xs:sequence>
			<xs:element name="Result">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>The Result Reporting Message Structure</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="ResultCode">
							<xs:annotation>
								<xs:documentation> Container type for the mandatory result code. Three different statuses: success, fail and reject. 
								 - Success is indicated with the value 0. 
								 - Failure(indicated that a resend could possible be successful) is represented by negative values 
								 - Rejection(indicated that a resend would be refected again) by positive values.
										</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer"/>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="ResultMsgCode">
							<xs:annotation>
								<xs:documentation>
						  Container type for the optional result message code. The recommended way is to concatenate the
						  system identifier with the specific error code number.This is only recommended and not enforced in the validation.
						</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="64"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="ResultMsg">
							<xs:annotation>
								<xs:documentation>
						  Container type for the optional detail description of the result message code.
						</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="Payload">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="EchoString" type="xs:string">
							<xs:annotation>
								<xs:documentation>Reference Number as supplied by Customer</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RequestType">
		<xs:sequence>
			<xs:element name="EchoIn" type="xs:string">
				<xs:annotation>
					<xs:documentation>Echo String</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
